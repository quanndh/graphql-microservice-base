{
  "name": "nest-base",
  "version": "0.0.1",
  "description": "base",
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "prebuild": "rimraf dist",
    "build": "node --max-old-space-size=4096 && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .ts,.js \"src/**\"",
    "test": "ts-node -C ttypescript ./test.ts",
    "type-check": "tsc --noEmit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/typeorm-migration.config.ts",
    "migrate:create": "yarn typeorm migration:create -n",
    "migrate:up": "yarn typeorm migration:run",
    "migrate:down": "yarn typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/common": "7.6.15",
    "@nestjs/core": "7.6.15",
    "@nestjs/cqrs": "7.0.1",
    "@nestjs/graphql": "10.1.7",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "7.6.15",
    "@nestjs/typeorm": "7.1.5",
    "apollo-server-express": "3.9.0",
    "apollo-server-plugin-base": "0.11.0",
    "axios": "0.21.1",
    "class-transformer": "^0.5.1",
    "class-validator": "0.13.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "dataloader": "2.0.0",
    "dayjs": "1.10.4",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "graphql": "16.5.0",
    "graphql-query-complexity": "0.8.0",
    "graphql-type-json": "0.3.2",
    "helmet": "4.5.0",
    "jwt-decode": "3.1.2",
    "nanoid": "3.1.22",
    "nestjs-typeorm-paginate": "2.3.2",
    "opaqueid": "1.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "4.0.0",
    "pg": "8.6.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.7",
    "ts-morph": "^17.0.1",
    "typeorm": "0.2.32",
    "typeorm-adapter": "1.4.0",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@apollo/federation": "^0.38.1",
    "@apollo/subgraph": "^2.2.2",
    "@nestjs/axios": "^0.0.8",
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/testing": "7.6.15",
    "@types/cache-manager": "^4.0.1",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.11",
    "@types/graphql-type-json": "0.3.2",
    "@types/jest": "26.0.22",
    "@types/jwt-decode": "3.1.0",
    "@types/lodash": "4.14.168",
    "@types/nanoid": "2.1.0",
    "@types/node": "14.14.41",
    "@types/passport-jwt": "3.0.5",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-security": "1.4.0",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "supertest": "6.1.3",
    "ts-jest": "26.5.5",
    "ts-node": "9.1.1",
    "tsc-watch": "4.2.9",
    "tsconfig-paths": "3.9.0",
    "ttypescript": "^1.5.13",
    "typescript": "4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
